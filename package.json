{
  "name": "dhive-mod",
  "version": "0.13.8",
  "description": "Hive blockchain RPC client library",
  "author": "tngflx",
  "license": "BSD-3-Clause",
  "main": "./lib/index-node",
  "typings": "./lib/index",
  "browser": {
    "./lib/index-node": "./dist/dhive.js",
    "./src/index-node": "./dist/dhive.js",
    "./src/version": "./lib/version.js"
  },
  "files": [
    "dist/*",
    "lib/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tngflx/dhive.git"
  },
  "scripts": {
    "start": "node lib/index.js",
    "prepublish": "yarn build",
    "dev": "ts-node-dev --respawn --transpileOnly --poll src/index.ts",
    "test": "mocha --require ts-node/register -r test/_node.js test/*.ts --grep 'crypto'",
    "build": "tsc -p tsconfig.json --outDir lib"
  },
  "keywords": [
    "blockchain",
    "client",
    "rpc",
    "hive"
  ],
  "dependencies": {
    "@types/bs58": "^4.0.1",
    "bs58": "^4.0.1",
    "bytebuffer": "^5.0.1",
    "core-js": "^3.6.4",
    "cross-fetch": "^3.0.4",
    "futoin-hkdf": "^1.3.2",
    "long": "^4.0.0",
    "node-fetch": "^2.6.0",
    "secp256k1": "^3.8.0",
    "tsc": "^1.20150623.0",
    "verror": "^1.10.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/preset-env": "^7.1.6",
    "@types/bytebuffer": "^5.0.41",
    "@types/long": "^4.0.1",
    "@types/mocha": "^5.2.5",
    "@types/node": "^13.13.5",
    "@types/verror": "^1.10.3",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "coveralls": "^3.0.11",
    "derequire": "^2.0.6",
    "dts-generator": "^2.1.0",
    "encoding": "^0.1.12",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "karma": "^6.3.14",
    "karma-browserify": "^5.1.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sauce-launcher": "^1.2.0",
    "lorem-ipsum": "^1.0.6",
    "mocha": "^7.1.2",
    "nyc": "^15.0.0",
    "regenerator-runtime": "^0.13.1",
    "ts-node": "^8.10.2",
    "tsify": "^4.0.0",
    "tslint": "^5.11.0",
    "typedoc": "^0.17.1",
    "typescript": "^3.8.3",
    "uglify-js": "^3.4.9",
    "watchify": "^3.11.0"
  }
